<h1>Listing customer_orders</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Date ordered</th>
    <th>Total</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @customer_orders.each do |customer_order| %>
  <tr>
    <td><%= customer_order.customer_id %></td>
    <td><%= customer_order.date_ordered %></td>
    <td><%= customer_order.total %></td>
    <td><%= link_to 'Show', customer_order %></td>
    <td><%= link_to 'Edit', edit_customer_order_path(customer_order) %></td>
    <td><%= link_to 'Destroy', customer_order, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

